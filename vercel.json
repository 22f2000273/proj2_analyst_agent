{
    "version": 2,
    "builds": [
      {
        "src": "*.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "env": {
      "GENAI_API_KEY": "@GENAI_API_KEY",
      "AIPIPE_TOKEN": "@AIPIPE_TOKEN"
    },
    "functions": {
      "main.py": {
        "maxDuration": 180
      }
    }
  }
  